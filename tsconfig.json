{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "moduleResolution": "bundler",
        "ignoreDeprecations": "6.0",
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "cypress/**/*.ts"
    ]
}